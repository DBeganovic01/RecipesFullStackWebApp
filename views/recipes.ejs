<%- include("partials/header") %>
        <div class="container">
            <h1 class="page-title">Recipes Page</h1>
                <div class="recipes-container">
                <% recipes.forEach((recipe) => { %>
                <div class="card">
                    <img class="card-img" src="<%= recipe.image %>" alt="Picture of the food">
                    <h3 class="card-recipeName"><%= recipe.recipeName %></h3>
                    <p class="card-description"><%= recipe.description %></p>
                </div>
                <% }) %>
            </div>
        </div>
<%- include("partials/footer") %>